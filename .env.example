# Database
DATABASE_URL=sqlite:///./botbuilder.db
# For PostgreSQL: postgresql://user:password@localhost/botbuilder

# Redis (optional, for session memory)
REDIS_URL=redis://localhost:6379
USE_REDIS=false

# Security
SECRET_KEY=your-secret-key-here-change-this
ADMIN_PASSWORD=changeme

# API URLs (for production)
API_BASE_URL=http://localhost:8000
FRONTEND_URL=http://localhost:3000

# CORS
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:8000

# Default AI Provider Keys (optional - can be set per bot)
DEFAULT_ANTHROPIC_API_KEY=
DEFAULT_OPENAI_API_KEY=

# Qdrant
QDRANT_URL=http://localhost:6333
QDRANT_API_KEY=
